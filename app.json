{
  "expo": {
    "name": "ankiApp",
    "slug": "ankiApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "deepLinking": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gallacyliga.ankiApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c15034e6-4b87-4fa4-9fe1-57ff6f0c4f84"
      }
    }
  }
}
